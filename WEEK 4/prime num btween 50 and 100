is_prime() {
        local num=$1
        if [ $num -le 1 ]; then
                return 1
        fi
        for(( i=2; i<=num / 2; i++)); do
                if(( num%i==0)); then
                        return 1
                fi
        done
        return 0
}
echo "Prime number between 50 and 100 are:"
for(( num=50; num<=100; num++)); do
        if is_prime $num; then
                echo $num
        fi
done


OUTPUT

Prime number between 50 and 100 are:
53
59
61
67
71
73
79
83
89
97
